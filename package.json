  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.9",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.22",
